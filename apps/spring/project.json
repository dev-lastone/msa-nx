{
  "name": "spring",
  "root": "apps/spring",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew build",
        "cwd": "apps/spring"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew bootRun",
        "cwd": "apps/spring"
      }
    }
  }
}
